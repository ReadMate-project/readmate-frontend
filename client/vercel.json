{
    "version": 2,
    "builds": [
      {
        "src": "serverless/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/pages/**/*.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/serverless/$1"
      }
    ]
  }
  