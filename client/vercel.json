{
    "version": 2,
    "builds": [
      {
        "src": "serverless/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/serverless/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "status": 200
      }
    ]
  }
  